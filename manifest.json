{
	"name": "XML Viewer",
	"version": "1.0.1",
	"description": "Formats remote, and local, XML documents for easy viewing.",
	"icons": { "16": "icon16.png",
			   "48": "icon48.png",
			  "128": "icon128.png" },
	"content_scripts": [ {
		"js": [ "treeBuilder.js", "document_end.1.js" ],
		"matches": [ "http://*/*", "https://*/*", "file:///*/*" ],
		"run_at": "document_end"
	} ]
}
